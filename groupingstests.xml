<?xml version="1.0" encoding="UTF-8"?>

<suite name= "OpenCart Test Suite" thread-count="2" parallel="tests" >  
	<listeners>
		<listener class-name="utilities.ExtentReportManager"/>
	</listeners>
	
	
	
	<test name="OpenCartTests">
		<groups>
			<run>
				<include name="master"/>
			</run>
		</groups>
	
	<parameter name="browser" value="chrome"/>
		<classes>
			 <class name ="testCases.TC_001_AccountRegistration"/> 
			<class name ="testCases.TC_002_LoginPage"/> 
			<class name ="testCases.TC_003_LoginDDT"/>	
			<class name ="testCases.TC_004_SearchBoxValidate"/> 
		</classes>
	</test>
	
	
</suite>